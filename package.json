{
  "name": "uptogo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "studio": "prisma studio",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,js,tsx,jsx,css,scss}\"",
    "watch": "next build tailwindcss -i ./styles/input.css -o ./dist/output.css --watch",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "engines": {
    "node": ">=12.x.x <=16.x.x",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@graphql-yoga/node": "2.12.0",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.8",
    "@next-auth/prisma-adapter": "1.0.4",
    "@prisma/client": "4.3.1",
    "@react-three/fiber": "8.6.2",
    "@reduxjs/toolkit": "1.8.3",
    "@splinetool/loader": "0.9.103",
    "@splinetool/r3f-spline": "1.0.1",
    "@splinetool/react-spline": "2.2.1",
    "@splinetool/runtime": "0.9.103",
    "@tailwindcss/line-clamp": "0.4.2",
    "axios": "0.27.2",
    "dotenv": "16.0.1",
    "framer-motion": "7.2.1",
    "graphql": "16.6.0",
    "next": "12.3.1",
    "next-auth": "4.12.3",
    "next-images": "1.8.4",
    "prisma": "4.3.1",
    "react": "18.2.0",
    "react-collapsible": "2.10.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "react-modal": "3.15.1",
    "react-redux": "8.0.2",
    "react-slick": "0.29.0",
    "react-slideshow-image": "4.0.5",
    "sharp": "0.31.0",
    "slick-carousel": "1.8.1",
    "three": "0.143.0",
    "uniqid": "5.4.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@graphql-codegen/typescript-resolvers": "2.7.1",
    "@next-auth/mongodb-adapter": "^1.0.4",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-modal": "3.13.1",
    "@types/react-scroll": "1.8.3",
    "@types/react-slick": "0.23.10",
    "@types/uuid": "8.3.4",
    "autoprefixer": "10.4.7",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.3",
    "husky": "8.0.0",
    "lint-staged": "13.0.3",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "tailwindcss": "3.1.2",
    "typescript": "4.8.2"
  },
  "lint-staged": {
    "**/*": "yarn prettier --write --ignore-unknown",
    "**/*.{scss,css}": "yarn stylelint --fix",
    "**/*.{ts,js,tsx,jsx}": "yarn eslint --fix"
  }

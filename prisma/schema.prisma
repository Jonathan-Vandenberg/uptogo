// ./prisma/schema.prisma

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

generator client {
  provider = "prisma-client-js"
}

model Account {
  id                String  @id @default(auto()) @map("_id") @db.ObjectId
  userId            String
  type              String
  provider          String
  providerAccountId String
  refresh_token     String? @db.String
  access_token      String? @db.String
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String? @db.String
  session_state     String?
  user              User    @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
}

model Session {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  sessionToken String   @unique
  userId       String
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model User {
  id            String    @id @default(auto()) @map("_id") @db.ObjectId
  name          String?
  email         String?   @unique
  emailVerified DateTime?
  image         String?
  accounts      Account[]
  sessions      Session[]
}

model VerificationToken {
  identifier String   @id @default(auto()) @map("_id") @db.ObjectId
  token      String   @unique
  expires    DateTime

  @@unique([identifier, token])
}

model It {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  softwareDevelopment   SoftwareDevelopment @relation(fields: [softwareDevelopmentId], references: [id])
  softwareDevelopmentId String              @db.ObjectId
  ComputerNetworking    ComputerNetworking? @relation(fields: [computerNetworkingId], references: [id])
  computerNetworkingId  String?             @db.ObjectId
  CloudComputing        CloudComputing?     @relation(fields: [cloudComputingId], references: [id])
  cloudComputingId      String?             @db.ObjectId
  BusinessAnalysis      BusinessAnalysis?   @relation(fields: [businessAnalysisId], references: [id])
  businessAnalysisId    String?             @db.ObjectId
  Telecommunication     Telecommunication?  @relation(fields: [telecommunicationId], references: [id])
  telecommunicationId   String?             @db.ObjectId
  GameProgramming       GameProgramming?    @relation(fields: [gameProgrammingId], references: [id])
  gameProgrammingId     String?             @db.ObjectId
  WebDevelopment        WebDevelopment?     @relation(fields: [webDevelopmentId], references: [id])
  webDevelopmentId      String?             @db.ObjectId
  MachineLearing        MachineLearing?     @relation(fields: [machineLearingId], references: [id])
  machineLearingId      String?             @db.ObjectId
  DataManagement        DataManagement?     @relation(fields: [dataManagementId], references: [id])
  dataManagementId      String?             @db.ObjectId
  Blockchain            Blockchain?         @relation(fields: [blockchainId], references: [id])
  blockchainId          String?             @db.ObjectId
  Ai                    Ai?                 @relation(fields: [aiId], references: [id])
  aiId                  String?             @db.ObjectId
}

